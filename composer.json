{
    "name": "workana/async-jobs",
    "description": "Job queue manager for multiple message queue systems",
    "type": "library",
    "license": "MIT",
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Workana\\AsyncJobs\\": "src/"
        },
        "exclude-from-classmap": ["tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Workana\\AsyncJobs\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Workana",
            "email": "support@workana.com"
        }
    ],
    "require": {
        "bernard/bernard": "dev-workana",
        "misterion/ko-process": "^0.5.3",
        "psr/container": "^1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:workana/bernard.git"
        }
    ],
    "prefer-stable": true
}
